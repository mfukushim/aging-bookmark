{
  "default_locale": "ja",
  "name": "__MSG_appName__",
  "description": "__MSG_description__",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "permissions": [
    "bookmarks",
    "history",
    "https://www.google-analytics.com/",
    "storage"
  ],
  "browser_action": {
    "default_title": "aging-bookmark"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
